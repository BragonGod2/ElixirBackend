# Port settings
PORT=3000

# Google OAuth Credentials
# You must create these in Google Cloud Console -> APIs & Services -> Credentials
# Redirect URI must be: <YOUR_HOSTINGER_DOMAIN>/auth/google/callback
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret
GOOGLE_REDIRECT_URI=https://your-hostinger-domain.com/auth/google/callback

# Session Secret (Optional, if using sessions later)
SESSION_SECRET=a_very_long_random_string

# Application URL (for internal redirects if needed)
APP_URL=https://your-hostinger-domain.com
